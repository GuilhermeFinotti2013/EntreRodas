@model IEnumerable<Web.Models.Veiculos>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Modelo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ano)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Placa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CategoriaCarro)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TipoCombustivel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TipoMotor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Observacoes)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Clientes.Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MarcasCarros.Nome)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Modelo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ano)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Placa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CategoriaCarro)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TipoCombustivel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TipoMotor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Observacoes)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Clientes.Nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MarcasCarros.Nome)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
